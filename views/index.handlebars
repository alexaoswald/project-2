<div class="section">
  <!-- main container -->
  <div class="container">
    <!-- title/subtitle box -->
    <div class="content">
      <h1 class="title">Welcome to Dogs' Day Out</h1>
      <h2 class="subtitle">A dog play date scheduler.</h2>
    </div>
    <!-- login form box -->
    <div class="container">
      <!-- user name field -->
      <div class="field">
        <label class="label">User Name</label>
        <div class="control">
          <input class="input" type="text" placeholder="Your user name here" />
        </div>
      </div>
      <!-- password field -->
      <div class="field">
        <label class="label">Password</label>
        <div class="control">
          <input
            class="input"
            type="password"
            placeholder="Your user name here"
          />
        </div>
      </div>
      <!-- form submit button -->
      <div class="control">
        <button class="button" type="submit">Log In</button>
      </div>
      <div class="control">
        <button class="button" id="setupBtn">Create Account</button>
      </div>
    </div>
  </div>
</div>

<script
  type="text/javascript"
  src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js">
  </script>
<script src="/js/index.js"></script>
